<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<TitledPane xmlns:fx="http://javafx.com/fxml/1" animated="false" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="480.0" prefWidth="640.0" text="MDHS"
            xmlns="http://javafx.com/javafx/21"
            fx:controller="au.edu.cqu.jhle.controllers.DeliveryScheduleDetailController">
    <content>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
            <children>
                <VBox layoutX="80.0" layoutY="49.0" prefHeight="290.0" prefWidth="530.0" AnchorPane.leftAnchor="50.0"
                      AnchorPane.rightAnchor="50.0">
                    <children>
                        <Label text="Postcode">
                            <VBox.margin>
                                <Insets top="10.0"/>
                            </VBox.margin>
                        </Label>
                        <TextField fx:id="postcodeInput" minWidth="-Infinity"/>
                        <Label text="Day">
                            <VBox.margin>
                                <Insets top="10.0"/>
                            </VBox.margin>
                        </Label>
                        <TextField fx:id="dayInput" minWidth="-Infinity"/>
                        <Label text="Cost">
                            <VBox.margin>
                                <Insets top="10.0"/>
                            </VBox.margin>
                        </Label>
                        <TextField fx:id="costInput" minWidth="-Infinity"/>
                    </children>
                </VBox>
                <Label fx:id="scheduleTitleLabel" layoutX="40.0" layoutY="21.0" text="New Schedule"
                       AnchorPane.leftAnchor="50.0" AnchorPane.topAnchor="10.0">
                    <font>
                        <Font size="16.0"/>
                    </font>
                </Label>
                <Button fx:id="onSaveDeliverySchedule" layoutX="80.0" layoutY="371.0" mnemonicParsing="false"
                        onAction="#onSaveDeliverySchedule" text="Save" AnchorPane.bottomAnchor="20.0"
                        AnchorPane.leftAnchor="50.0"/>
                <Button layoutX="514.0" layoutY="374.0" mnemonicParsing="false" onAction="#onReturnToList" text="Cancel"
                        AnchorPane.bottomAnchor="20.0" AnchorPane.rightAnchor="50.0"/>
            </children>
        </AnchorPane>
    </content>
    <font>
        <Font size="19.0"/>
    </font>
</TitledPane>